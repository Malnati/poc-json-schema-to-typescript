import { Edit, SimpleForm, <%=
    Array.from(new Set(fields.map(field => field.component))).join(', ')
%> } from 'react-admin';

export const <%= name %>Edit = () => (
    <Edit>
        <SimpleForm>
            <% fields.forEach(field => { %>
                <<%= field.component %> source="<%= field.name %>" />
            <% }) %>
        </SimpleForm>
    </Edit>
);
